{
  "name": "garethellis/wordy",
  "description": "A package to help get the properties of Word documents.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gareth Ellis",
      "email": "garethellis@gmail.com"
    }
  ],
  "require": {
    "php": "8.0.*",
    "ext-zip": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "test": "docker run wordy phpunit"
  },
  "autoload": {
    "psr-4": {
      "Garethellis\\Wordy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Garethellis\\Wordy\\Test\\": "test/"
    }
  }
}
